{
    "name": "lavakit/framework",
    "description": "The Lavakit framework",
    "keywords": ["lavakit", "framework", "lavakit framework"],
    "license": "MIT",
    "homepage": "http://lavakit.info",
    "support": {
        "issues": "https://github.com/lavakit/lavakit/issues",
        "document": "http://lavakit.readthedocs.io"
    },
    "authors": [
        {
            "name": "Trinh Quoc Hoa",
            "email": "tqhoa8th@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~5.8.0||^6.0||^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "hassankhan/config": "^2.0",
        "symfony/yaml": "^5.0",
        "mcamara/laravel-localization": "^1.5",
        "laravel/passport": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "autoload": {
        "classmap": [
            "modules/user/database/seeds"
        ],
        "psr-4": {
            "Lavakit\\Base\\": "modules/base/src",
            "Lavakit\\Theme\\": "modules/theme/src",
            "Lavakit\\Dashboard\\": "modules/dashboard/src",
            "Lavakit\\Page\\": "modules/page/src",
            "Lavakit\\User\\": "modules/user/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lavakit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lavakit\\Base\\Providers\\BaseServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
